{
  "name": "@morfeo/cli",
  "description": "a command line interface to use morfeo",
  "version": "0.0.0",
  "author": "Mauro Erta <mauro.erta@gmail.com>",
  "bin": {
    "morfeo": "./bin/run"
  },
  "bugs": "https://github.com/VLK-STUDIO/morfeo/issues",
  "dependencies": {
    "@morfeo/web": "^0.1.6",
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.2",
    "style-dictionary": "^3.0.1",
    "tslib": "^1.14.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@types/node": "^10.17.60",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.10"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://morfeo.dev",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "morfeo",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "https://github.com/VLK-STUDIO/morfeo",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
